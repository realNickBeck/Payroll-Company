    <header class="bg-primary text-white text-center">
        <h1 class="m-3 p-3">My Projects</h1>
    </header>
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Date Assigned</th>
