@model List<Employee>
@{
    ViewBag.Title = "My Account";
}
<div class="col-sm-12">
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>My Information</th>
        </thead>
        <tbody>
            <tr>
                <td>Username: </td>
                <td>Employee ID: </td>
                <td>Employee Title: </td>
            </tr>
            <tr>
                <td>First Name: </td>
                <td>Last Name: </td>
            </tr>
            <tr>
                <td>Department ID: </td>
                <td>Department: </td>
            </tr>
            <tr>
                <td>Address: </td>
                <td>City: </td>
